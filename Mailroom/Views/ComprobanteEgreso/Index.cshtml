<!DOCTYPE html>
<html lang="en" ng-app="core" ng-controller="ComprobanteEgresoController">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" charset="utf-8" />
    <link rel="icon" type="image/png" href="/mailroom/Content/img/fav.png" />
    <!-- Bootstrap CSS -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link type="text/css" rel="stylesheet" media="screen,projection" />
    <title>OCASA</title>
</head>
<body>
    <div id="divLoading"></div>
    <div class="row">
        <div class="col-md-6">
            <h2><i class="fas fa-external-link-alt"></i>&nbsp;Comprobante Egreso</h2>
        </div>
        <div class="col-md-6 float-right hide-print">
            <a class="float-right" href="javascript:window.print();"><h4><i class="material-icons">print</i>&nbsp;</h4></a>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="table-responsive">
                <table class="table table-striped ">
                    <thead>
                        <tr>
                            <th scope="col">Nro. Seguimiento</th>
                            <th scope="col">Fecha</th>
                            <th scope="col">Producto</th>
                            <th scope="col">Proveedor</th>
                            <th scope="col">Autorizado</th>
                            <th scope="col">Destinatario</th>
                            <th scope="col">Sector</th>
                            <th scope="col">Dirección</th>
                            <th scope="col">Estado</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="itemEn in itemsEgreso">
                            <td>{{itemEn.NroSeguimiento}}</td>
                            <td>{{itemEn.Fecha}}</td>
                            <td>{{itemEn.TipoProducto}}</td>
                            <td>{{itemEn.Proveedor}}</td>
                            <td>{{itemEn.AutorizadoRetirar}}</td>
                            <td>{{itemEn.Destinatario}}</td>
                            <td>{{itemEn.Sector}}</td>
                            <td>{{itemEn.Direccion}}</td>
                            <td>{{itemEn.Estado}}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrapjs")
    @Styles.Render("~/Content/bootstrap")


    @Scripts.Render("~/Angular/js")

    @Scripts.Render("~/AngularVendor/js")
    @Styles.Render("~/AngularVendor/css")
    @Scripts.Render("~/Pagination/js")


    @Scripts.Render("~/bundles/uibootstrapjs")
    @Scripts.Render("~/angular/core")
    @Styles.Render("~/Common/css")
    @Scripts.Render("~/Common/js")


    @Styles.Render("~/Vendor/css")
    @Scripts.Render("~/Vendor/js")
</body>
</html>